{
  "name": "reisekosten-web-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:pact": "vue-cli-service test:unit --runInBand --testMatch **/*.pact.ts",
    "lint": "vue-cli-service lint",
    "pact:publish": "pact-broker publish pact/pacts --consumer-app-version=\"$(npx @pact-foundation/absolute-version)\" --auto-detect-version-properties --broker-base-url=https://pact.grossweber.com",
    "pact:can-i-deploy": "pact-broker can-i-deploy --pacticipant 'Travel Expenses Web UI' --latest --broker-base-url=https://pact.grossweber.com"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "core-js": "^3.8.3",
    "vue": "^3.2.13",
    "vue-class-component": "^8.0.0-0",
    "vue-property-decorator": "^9.1.2"
  },
  "devDependencies": {
    "@pact-foundation/absolute-version": "^0.0.4",
    "@pact-foundation/pact": "^9.17.3",
    "@types/jest": "^27.4.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "babel-jest": "^27.0.6",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.0.5",
    "jest-pact": "^0.9.3",
    "supertest": "^6.2.2",
    "ts-jest": "^27.0.4",
    "typescript": "~4.5.5"
  }
}
